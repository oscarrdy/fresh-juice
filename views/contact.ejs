          <p class="error_text" id="error_question"></p>
          <textarea name="question" id="inp_question" cols="30" rows="10" placeholder="Write your question here."></textarea>
        </div>
        <div class="footer">
          <button type="submit" id="btn_submit_question" class="btn_submit_question">
            Submit Question
            <span class="loading_dots"><span></span><span></span><span></span></span>
          </button>
